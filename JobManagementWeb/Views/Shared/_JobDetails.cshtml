@model JobManagementWeb.Infrastructure.Models.JobVM

<!-- Display the job details-->

<div class="card-header">
    <h4>Job Details</h4>
    <span id="spanJobId" class="jobDetails">
    </span>
</div>
<div class="card-body">
    <div>
        <dl>
            <dt>
                <span id="spanJobId" class="jobDetails">
                </span>
            </dt>
            <dt>
                <span id="spanCustomerName" class="jobDetails">
                </span>
                (<span id="spanCustomerPhone" class="jobDetails"></span>)
            </dt>
            <dt>
                <span id="spanServiceType" class="jobDetails">
                </span>
            </dt>
            <dt>
                @Html.DisplayNameFor(model => model.AssignedTo)
                <span id="spanAssignedTo" class="jobDetails">
                </span>
            </dt>
            <dt>
                <span id="spanStatus" class="jobDetails">
                </span>
            </dt>
        </dl>
    </div>
</div>