@model List<JobManagementWeb.Infrastructure.Models.JobVM>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Jobs</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="">
                <td>
                    @item.JobId
                </td>
                <td>
                    @item.CustomerName
                </td>
                <td>
                    @item.CustomerPhone
                </td>
                <td>
                    @item.ServiceType
                </td>
                <td>
                    @Html.ActionLink("Assign", "Assign", new { id=item.JobId })
                </td>
            </tr>
        }
    </tbody>
</table>

